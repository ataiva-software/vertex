<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Example suppression - replace with actual suppressions as needed -->
    <!--
    <suppress>
        <notes>This suppresses CVE-XXXX-YYYY in the specified library because we're using it in a way that's not affected by the vulnerability</notes>
        <cve>CVE-XXXX-YYYY</cve>
        <gav>group:artifact:version</gav>
    </suppress>
    -->
    
    <!-- Suppress false positives -->
    <suppress>
        <notes>This is a false positive for our implementation</notes>
        <cpe>cpe:/a:apache:log4j</cpe>
        <packageUrl regex="true">^pkg:maven/org\.apache\.logging\.log4j/log4j-api@2\.17\..*$</packageUrl>
    </suppress>
    
    <!-- Suppress vulnerabilities in test dependencies -->
    <suppress>
        <notes>These are test dependencies and not used in production</notes>
        <filePath regex="true">.*[/\\]test[/\\].*</filePath>
    </suppress>
    
    <!-- Suppress vulnerabilities in development tools -->
    <suppress>
        <notes>These are development tools and not used in production</notes>
        <filePath regex="true">.*[/\\]build-tools[/\\].*</filePath>
    </suppress>
</suppressions>